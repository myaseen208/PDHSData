##----PKBR21----
PKBR21 <-
stata.get(
file                = "Stata/PKBR21FL.DTA"
, lowernames          = FALSE
, convert.dates       = TRUE
, convert.factors     = TRUE
, missing.type        = FALSE
, convert.underscore  = TRUE
, warn.missing.labels = TRUE
, force.single        = TRUE
, allow               = NULL
, charfactor          = FALSE
) %>%
as_tibble()
# PKBR21
save(
#  ...
PKBR21
, file              = "R/PKBR21.RData"
, ascii             = FALSE
, version           = NULL
, envir             = parent.frame()
# , compress          = "xz"
#  , compression_level
, eval.promises     = TRUE
, precheck          = TRUE
)
load("R/PKBR21.RData")
PKBR21
methods(class = class(PKBR21))
label(PKBR21)
describe(PKBR21)
PKBR21tb1 <-
tibble(
Names = attributes(PKBR21)$names
, Labels = label(PKBR21)
) %>%
dplyr::mutate(
Code =
paste0(
paste0("\\item{\\code{", Names, "}}")
, paste0("{", Labels, "}")
)
)
PKBR21tb1 %>%
dplyr::select(Code)
PKBR21tb1Code <-
PKBR21tb1 %>%
dplyr::select(Code) %>%
as.data.frame() %>%
format(justify = "left") %>%
print(row.names = FALSE)
write_csv(x = PKBR21tb1Code, path = "Code/PKBR21tb1Code.csv")
